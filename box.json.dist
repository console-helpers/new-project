{
    "alias": "{example-app}.phar",
    "chmod": "0755",
    "directories": ["src", "bin"],
    "finder": [
        {
            "name": "*.php",
            "exclude": [
                "tests",
                "*Test.php",
                "test",
                "Tests",
                "Test",
                "*.lock"
            ],
            "in": "vendor"
        }
    ],
	"blacklist": ["vendor/aik099/coding-standard"],
    "main": "bin/{example-app}",
	"git-version": "git-version",
    "output": "build/{example-app}.phar",
    "stub": true
}
